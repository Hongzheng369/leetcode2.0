3rd
class Solution {
    public int myAtoi(String str) {
        int index = 0;
        int total = 0;
        int sign = 1;
        
        // Check if empty string
        if(str.length() == 0)
            return 0;
        
        // remove white spaces from the string
        while(index < str.length() && str.charAt(index) == ' ')
            index++;
        
        if (index == str.length()) return 0;
        
        // get the sign
        if(str.charAt(index) == '+' || str.charAt(index) == '-') {
            sign = str.charAt(index) == '+' ? 1 : -1;
            index++;
        }
        
        // convert to the actual number and make sure it's not overflow
        while(index < str.length()) {
            int digit = str.charAt(index) - '0';
            if(digit < 0 || digit > 9) break;
            
            // check for overflow
            if(Integer.MAX_VALUE / 10 < total || Integer.MAX_VALUE / 10 == total && Integer.MAX_VALUE % 10 < digit)
                return sign == 1 ? Integer.MAX_VALUE : Integer.MIN_VALUE;
            
            total = total*10 + digit;
            index++; // don't forget to increment the counter
        }
        return total*sign;
    }
}

^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^


class Solution {
    public int myAtoi(String str) {
    if (str.length() < 1) {
    return 0;
}

	int result = 0;
	int sign = 1;
	int firstIndex = 0;
	while (firstIndex < str.length()) {
		Character c = str.charAt(firstIndex);
		firstIndex++;
		if (Character.isDigit(c) ) {
			result = c - 48;
			break;
		} else if (c == 45) {
			sign = -1;
			break;
		} else if (c == 43){
            break;
        }else if (c != 32){
            return 0;
        }
	}
	for (int i = firstIndex; i < str.length(); i++) {
		Character c = str.charAt(i);
		if (!Character.isDigit(c)) {
			return result * sign;
		}
		int temp = result * 10 + c - 48;
		if (temp/10 != result) {
			if (sign == 1) {
				return Integer.MAX_VALUE;
			} else {
				return Integer.MIN_VALUE;
			}
			}
		}
		result = temp;
	}
	return result * sign;
}
}



///2nd time 
class Solution {
    public int myAtoi(String str) {
    if (str.length() < 1) {
    return 0;
}

	int result = 0;
	int sign = 1;
	int firstIndex = 0;
	while (firstIndex < str.length()) {
		Character c = str.charAt(firstIndex);
		firstIndex++;
		if (Character.isDigit(c) ) {
			result = c - 48;
			break;
		} else if (c == 45) {
			sign = -1;
			break;
		} else if (c == 43){
            break;
        }else if (c != 32){
            return 0;
        }
	}
	for (int i = firstIndex; i < str.length(); i++) {
		Character c = str.charAt(i);
		if (!Character.isDigit(c)) {
			return result * sign;
		}
		int temp = result * 10 + c - 48;
		if (temp/10 != result) {
			if (sign == 1) {
				return Integer.MAX_VALUE;
			} else {
				return Integer.MIN_VALUE;
			}
			}
		}
		result = temp;
	}
	return result * sign;
}
}



