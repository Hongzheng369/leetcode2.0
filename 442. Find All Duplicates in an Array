class Solution {
   public List<Integer> findDuplicates(int[] nums) {
        List<Integer> result = new ArrayList<>();
        for(int i = 0; i < nums.length; i++){
            while(nums[i] - 1 != i){
                int temp = nums[nums[i] - 1];
                
                if(temp == nums[i])
                {break;}
                nums[nums[i] - 1] = nums[i];
                nums[i] = temp;
            }
        }
        for(int i = 0; i < nums.length; i++)
            if(nums[i] - 1 != i) 
            {result.add(nums[i]);}
       return result;
    }
}
