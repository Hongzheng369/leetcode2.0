class Solution {
    public int singleNumber(int[] nums) {
    Arrays.sort(nums);
    if(nums.length == 0 ) return 0;
    for(int i = 0; i < nums.length-2; i++ ){
         if(nums[i] != nums[i+1] || nums[i] != nums[i+2]){
             return nums[i];
         }else{
             i += 2;;
         }
    }
       return nums[nums.length-1];
}
}
